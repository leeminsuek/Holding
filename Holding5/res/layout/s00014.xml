                    android:layout_marginLeft="15dip"
                    android:layout_toRightOf="@id/img2_layout"
                    android:text="알림"
                    android:textColor="#3e3e3f" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.3dip"
                android:background="#eeeeee" />

            <RelativeLayout
