<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
         android:focusableInTouchMode="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/title_bg" >

        <ImageView
            android:id="@+id/header_back_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:clickable="true"
            android:padding="5dp"
            android:src="@drawable/s00116_back_button" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:gravity="center" >

            <ImageView
                android:id="@+id/header_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/title_logo_img" />

            <TextView
                android:id="@+id/header_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#818181" />
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#eaeff3"
        android:orientation="vertical" >

        <ScrollView
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="10dip"
            android:layout_weight="1"
            android:fillViewport="true" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <!--
                     <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >



                -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/content_box_bg"
                    android:orientation="vertical" >

                    <FrameLayout
                        android:id="@+id/profileimglayout"
                        android:layout_width="60dip"
                        android:layout_height="60dip"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="10dip" >

                        <ImageView
                            android:id="@+id/profileimg"
                            android:layout_width="60dip"
                            android:layout_height="60dip"
                            android:layout_centerHorizontal="true" />

                        <ImageView
                            android:layout_width="60dip"
                            android:layout_height="60dip"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/prf_box_bg" />
                    </FrameLayout>

                    <RelativeLayout
                        android:id="@+id/profile1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/profileimglayout"
                        android:layout_marginLeft="10dip"
                        android:layout_marginRight="10dip"
                        android:layout_marginTop="20dip"
                        android:background="@drawable/input_box_bg" >

                        <ImageView
                            android:visibility="gone"
                            android:id="@+id/modifybtn1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/help_prf_modify_btn" />

                        <TextView
                            android:id="@+id/textview1"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="닉네임" />

                        <TextView
                            android:id="@+id/edittext1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/modifybtn1"
                            android:layout_toRightOf="@id/textview1"
                            android:background="#00000000"
                            android:singleLine="true"
                           
                            android:text=""
                            android:textSize="12sp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profile2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/profile1"
                        android:layout_marginLeft="10dip"
                        android:layout_marginRight="10dip"
                        android:layout_marginTop="10dip"
                        android:background="@drawable/input_box_bg" >

                        <ImageView
                            android:id="@+id/modifybtn2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/help_prf_modify_btn" />

                        <TextView
                            android:id="@+id/textview2"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="비밀번호" />

                        <TextView
                            android:id="@+id/edittext2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/modifybtn2"
                            android:layout_toRightOf="@id/textview2"
                            android:background="#00000000"
                            android:password="true"
                            android:singleLine="true"
                            android:text=""
                             android:focusable="false"
                            android:textSize="12sp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profile3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/profile2"
                        android:layout_marginLeft="10dip"
                        android:layout_marginRight="10dip"
                        android:layout_marginTop="10dip"
                        android:background="@drawable/input_box_bg" >

                        <ImageView
                            android:id="@+id/modifybtn3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/help_prf_modify_btn" />

                        <TextView
                            android:id="@+id/textview3"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="생년월일" />

                        <TextView
                            android:id="@+id/edittext3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/modifybtn3"
                            android:layout_toRightOf="@id/textview3"
                            android:background="#00000000"
                            android:singleLine="true"
                            android:text=""
                             android:focusable="false"
                            android:textSize="12sp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profile4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/profile3"
                        android:layout_marginLeft="10dip"
                        android:layout_marginRight="10dip"
                        android:layout_marginTop="10dip"
                        android:background="@drawable/input_box_bg" >

                        <ImageView
                            android:id="@+id/modifybtn4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/help_prf_modify_btn" />

                        <TextView
                            android:id="@+id/textview4"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="학교" />

                        <EditText
                            android:id="@+id/edittext4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/modifybtn4"
                            android:layout_toRightOf="@id/textview4"
                            android:background="#00000000"
                            android:singleLine="true"
                            android:text=""
                            android:textSize="12sp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profile5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/profile4"
                        android:layout_marginLeft="10dip"
                        android:layout_marginRight="10dip"
                        android:layout_marginTop="10dip"
                        android:background="@drawable/input_box_bg" >

                        <ImageView
                            android:id="@+id/modifybtn5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/help_prf_modify_btn" />

                        <TextView
                            android:id="@+id/textview5"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="지역" />

                        <EditText
                            android:id="@+id/edittext5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/modifybtn5"
                            android:layout_toRightOf="@id/textview5"
                            android:background="#00000000"
                            android:singleLine="true"
                            
                            android:text=""
                            android:textSize="12sp" />
                    </RelativeLayout>
                    <!-- </RelativeLayout> -->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dip"
                        android:gravity="center"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/tab1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/tap_menu_01_dw" />

                        <ImageView
                            android:id="@+id/tab2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/tap_menu_02" />
                    </LinearLayout>

                    <ListView
                        android:id="@+id/listview"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@id/profile5"
                        android:layout_weight="1"
                        android:cacheColorHint="#00000000"
                        android:divider="#eeeeee"
                        android:dividerHeight="0.3dp"
                        android:clickable="false"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:longClickable="false"
                        android:scrollbars="none" >
                    </ListView>
                </LinearLayout>

                <ImageButton
                    android:id="@+id/morebtn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dip"
                    android:background="@color/clear"
                    android:src="@drawable/selector_help_more_btn"
                     />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</LinearLayout>