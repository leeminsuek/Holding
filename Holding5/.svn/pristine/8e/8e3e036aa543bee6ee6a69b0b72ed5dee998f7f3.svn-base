<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eaeff3"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/relativelayout_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/title_bg" >

        <ImageView
            android:id="@+id/imageView_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:clickable="true"
            android:padding="5dp"
            android:src="@drawable/s00116_back_button" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center" >

            <ImageView
                android:id="@+id/header_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/title_logo_img" />

            <TextView
                android:id="@+id/header_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="희망 메세지 수정"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#818181" />
        </LinearLayout>
    </RelativeLayout>

    <ImageView
        android:id="@+id/header_shadow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/relativelayout_top"
        android:src="@drawable/title_bg_sdw" />

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativelayout_top" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="@dimen/dimen10dp" >

            <RelativeLayout
                android:id="@+id/group_1_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/write_bg_box_group_01" />

                <TextView
                    android:id="@+id/textView1"
                    style="@style/write_menu_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="@dimen/dimen10dp"
                    android:text="분류" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/group_1_bottom_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/content_box_bg_center" >

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/dimen10dp"
                    android:paddingLeft="@dimen/dimen15dp"
                    android:paddingTop="@dimen/dimen10dp" >

                    <RadioButton
                        android:id="@+id/radio_group_1"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="공통" />

                    <RadioButton
                        android:id="@+id/radio_group_2"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="성적, 학업문제" />

                    <RadioButton
                        android:id="@+id/radio_group_3"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="부모님과 갈등" />

                    <RadioButton
                        android:id="@+id/radio_group_4"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="가정형편, 경제" />

                    <RadioButton
                        android:id="@+id/radio_group_5"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="기타문제" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/dimen10dp"
                    android:paddingLeft="@dimen/dimen10dp"
                    android:paddingTop="@dimen/dimen10dp" >

                    <RadioButton
                        android:id="@+id/radio_group_6"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="왕따, 학교폭력" />

                    <RadioButton
                        android:id="@+id/radio_group_7"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="친구, 이성문제" />

                    <RadioButton
                        android:id="@+id/radio_group_8"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="선생님과 갈등" />

                    <RadioButton
                        android:id="@+id/radio_group_9"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="외모문제" />

                    <RadioButton
                        android:id="@+id/radio_group_10"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="극복수기" />
                </LinearLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/group_3_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/write_bg_box_group_03" />

                <TextView
                    style="@style/write_menu_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="@dimen/dimen10dp"
                    android:text="종교" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/group_3_bottom_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/content_box_bg_center" >

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/dimen10dp"
                    android:paddingLeft="@dimen/dimen15dp"
                    android:paddingTop="@dimen/dimen10dp" >

                    <RadioButton
                        android:id="@+id/radio_group2_1"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="상관없음" />

                    <RadioButton
                        android:id="@+id/radio_group2_2"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="천주교인" />

                    <RadioButton
                        android:id="@+id/radio_group2_3"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="무교" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/dimen10dp"
                    android:paddingLeft="@dimen/dimen10dp"
                    android:paddingTop="@dimen/dimen10dp" >

                    <RadioButton
                        android:id="@+id/radio_group2_4"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="기독교인" />

                    <RadioButton
                        android:id="@+id/radio_group2_5"
                        style="@style/search_radio_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen5dp"
                        android:button="@drawable/selector_main_search_check"
                        android:text="불교인" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ico_group_line" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/content_box_bg_center"
                android:padding="@dimen/dimen10dp" >

                <EditText
                    android:id="@+id/editText_contents"
                    style="@style/write_contents_text"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:background="@drawable/write_input_blue_bg"
                    android:gravity="top"
                    android:padding="@dimen/dimen10dp" />
            </RelativeLayout>

            <HorizontalScrollView
                android:id="@+id/horizontalScrollView1"
                android:layout_width="match_parent"
                 android:background="@drawable/content_box_bg_center"
                android:visibility="gone"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:id="@+id/add_picture_layout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                   >

                    <com.br.holding5.sc003.gallery.DeleteImageView
                        android:id="@+id/photo_view_1"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginRight="@dimen/dimen5dp" >
                    </com.br.holding5.sc003.gallery.DeleteImageView>

                    <com.br.holding5.sc003.gallery.DeleteImageView
                        android:id="@+id/photo_view_2"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginRight="@dimen/dimen5dp" >
                    </com.br.holding5.sc003.gallery.DeleteImageView>

                    <com.br.holding5.sc003.gallery.DeleteImageView
                        android:id="@+id/photo_view_3"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginRight="@dimen/dimen5dp" >
                    </com.br.holding5.sc003.gallery.DeleteImageView>

                    <com.br.holding5.sc003.gallery.DeleteImageView
                        android:id="@+id/photo_view_4"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginRight="@dimen/dimen5dp" >
                    </com.br.holding5.sc003.gallery.DeleteImageView>

                    <com.br.holding5.sc003.gallery.DeleteImageView
                        android:id="@+id/photo_view_5"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginRight="@dimen/dimen5dp" >
                    </com.br.holding5.sc003.gallery.DeleteImageView>

                    <com.br.holding5.sc003.gallery.DeleteImageView
                        android:id="@+id/photo_view_6"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginRight="@dimen/dimen5dp" >
                    </com.br.holding5.sc003.gallery.DeleteImageView>

                    <com.br.holding5.sc003.gallery.DeleteImageView
                        android:id="@+id/photo_view_7"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginRight="@dimen/dimen5dp" >
                    </com.br.holding5.sc003.gallery.DeleteImageView>
                </LinearLayout>
            </HorizontalScrollView>

            <View
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ico_group_line" />


            <View
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ico_group_line" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:background="@drawable/content_box_bg_center"
                android:orientation="horizontal" >

                <ImageButton
                    android:id="@+id/modify_btn"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/clear"
                    android:src="@drawable/selector_btn_modify2" />

                <ImageButton
                    android:id="@+id/delete_btn"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/clear"
                    android:src="@drawable/selector_btn_delete" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>