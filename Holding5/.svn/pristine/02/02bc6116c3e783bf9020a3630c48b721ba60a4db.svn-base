<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/relativeLayout1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen50dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:background="@drawable/title_bg" >

                <ImageButton
                    android:id="@+id/write_back_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen5dp"
                    android:background="@color/clear"
                    android:src="@drawable/selector_detail_back_btn" />

                <LinearLayout
                    android:id="@+id/LinearLayout1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true" >

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/title_logo_img" />

                    <TextView
                        android:id="@+id/textView2"
                        style="@style/common_title_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/dimen15dp"
                        android:text="글쓰기" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/content_box_bg_center"
                    android:orientation="vertical"
                    android:padding="@dimen/dimen10dp" >

                    <RelativeLayout
                        android:id="@+id/group_1_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="visible" >

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/write_bg_box_group_01" />

                        <TextView
                            android:id="@+id/textView1"
                            style="@style/write_menu_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:paddingLeft="@dimen/dimen10dp"
                            android:text="어떤 문제가 있나요?" />
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/group_1_bottom_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/content_box_bg_center"
                        android:visibility="visible" >

                        <LinearLayout
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/dimen10dp"
                            android:paddingLeft="@dimen/dimen15dp"
                            android:paddingTop="@dimen/dimen10dp" >

                            <RadioButton
                                android:id="@+id/radio_group_1"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:checked="true"
                                android:text="가벼운 이야기" />

                            <RadioButton
                                android:id="@+id/radio_group_3"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="성적, 학업문제" />

                            <RadioButton
                                android:id="@+id/radio_group_5"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="부모님과 갈등" />

                            <RadioButton
                                android:id="@+id/radio_group_7"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="가정형편, 경제" />

                            <RadioButton
                                android:id="@+id/radio_group_9"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="기타문제" />

                            <RadioButton
                                android:id="@+id/radio_group_11"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="긴급SOS" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/dimen10dp"
                            android:paddingLeft="@dimen/dimen10dp"
                            android:paddingTop="@dimen/dimen10dp" >

                            <RadioButton
                                android:id="@+id/radio_group_2"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="왕따, 학교폭력" />

                            <RadioButton
                                android:id="@+id/radio_group_4"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="친구, 이성문제" />

                            <RadioButton
                                android:id="@+id/radio_group_6"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="선생님과 갈등" />

                            <RadioButton
                                android:id="@+id/radio_group_8"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="외모문제" />

                            <RadioButton
                                android:id="@+id/radio_group_10"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="극복수기" />
                        </LinearLayout>
                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/group_2_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="visible" >

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/write_bg_box_group_02" />

                        <TextView
                            style="@style/write_menu_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:paddingLeft="@dimen/dimen10dp"
                            android:text="얼마나 심각한가요?" />
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/group_2_bottom_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/content_box_bg_center"
                        android:visibility="visible" >

                        <LinearLayout
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/dimen10dp"
                            android:paddingLeft="@dimen/dimen15dp"
                            android:paddingTop="@dimen/dimen10dp" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="상황의 심각성"
                                android:textColor="#000000" />

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_toRightOf="@+id/ico_s_menu_btn"
                                android:background="@drawable/ico_group_line" >
                            </FrameLayout>

                            <RadioButton
                                android:id="@+id/radio_group1_1"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:checked="true"
                                android:text="괜찮아요" />

                            <RadioButton
                                android:id="@+id/radio_group1_2"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="약간 심해요" />

                            <RadioButton
                                android:id="@+id/radio_group1_3"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="심각해요" />

                            <RadioButton
                                android:id="@+id/radio_group1_4"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="많이 심각해요" />

                            <RadioButton
                                android:id="@+id/radio_group1_5"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="최고로 심각해요" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/dimen10dp"
                            android:paddingLeft="@dimen/dimen10dp"
                            android:paddingTop="@dimen/dimen10dp" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="마음의 상태"
                                android:textColor="#000000" />

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:layout_marginTop="@dimen/dimen5dp"
                                android:layout_toRightOf="@+id/ico_s_menu_btn"
                                android:background="@drawable/ico_group_line" >
                            </FrameLayout>

                            <RadioButton
                                android:id="@+id/radio_group1_1_1"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:checked="true"
                                android:text="괜찮아요" />

                            <RadioButton
                                android:id="@+id/radio_group1_1_2"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="약간 불안해요" />

                            <RadioButton
                                android:id="@+id/radio_group1_1_3"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="불안해요" />

                            <RadioButton
                                android:id="@+id/radio_group1_1_4"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="많이 불안해요" />

                            <RadioButton
                                android:id="@+id/radio_group1_1_5"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="죽을만큼 불안해요" />
                        </LinearLayout>
                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/group_3_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/write_bg_box_group_03" />

                        <TextView
                            style="@style/write_menu_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:paddingLeft="@dimen/dimen10dp"
                            android:text="의지할 종교는 있나요?" />
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/group_3_bottom_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/content_box_bg_center" >

                        <LinearLayout
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/dimen10dp"
                            android:paddingLeft="@dimen/dimen15dp"
                            android:paddingTop="@dimen/dimen10dp" >

                            <RadioButton
                                android:id="@+id/radio_group2_1"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:checked="true"
                                android:text="상관없음" />

                            <RadioButton
                                android:id="@+id/radio_group2_3"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="천주교인" />

                            <RadioButton
                                android:id="@+id/radio_group2_5"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="무교" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/dimen10dp"
                            android:paddingLeft="@dimen/dimen10dp"
                            android:paddingTop="@dimen/dimen10dp" >

                            <RadioButton
                                android:id="@+id/radio_group2_2"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="기독교인" />

                            <RadioButton
                                android:id="@+id/radio_group2_4"
                                style="@style/search_radio_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen5dp"
                                android:button="@drawable/selector_main_search_check"
                                android:text="불교인" />
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ico_group_line" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/content_box_bg_center"
                        android:padding="@dimen/dimen10dp" >

                        <EditText
                            android:id="@+id/add_board_content_txt"
                            style="@style/write_contents_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/write_input_blue_bg"
                            android:gravity="top"
                            android:minHeight="150dp"
                            android:padding="@dimen/dimen10dp" >
                        </EditText>
                    </RelativeLayout>

                    <HorizontalScrollView
                        android:id="@+id/horizontalScrollView1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/content_box_bg_center"
                        android:paddingBottom="@dimen/dimen10dp"
                        android:paddingLeft="@dimen/dimen10dp"
                        android:paddingRight="@dimen/dimen10dp" >

                        <LinearLayout
                            android:id="@+id/add_picture_layout"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:orientation="horizontal"
                            android:visibility="gone" >

                            <com.br.holding5.sc003.gallery.DeleteImageView
                                android:id="@+id/photo_view_1"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginRight="@dimen/dimen5dp" >
                            </com.br.holding5.sc003.gallery.DeleteImageView>

                            <com.br.holding5.sc003.gallery.DeleteImageView
                                android:id="@+id/photo_view_2"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginRight="@dimen/dimen5dp" >
                            </com.br.holding5.sc003.gallery.DeleteImageView>

                            <com.br.holding5.sc003.gallery.DeleteImageView
                                android:id="@+id/photo_view_3"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginRight="@dimen/dimen5dp" >
                            </com.br.holding5.sc003.gallery.DeleteImageView>

                            <com.br.holding5.sc003.gallery.DeleteImageView
                                android:id="@+id/photo_view_4"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginRight="@dimen/dimen5dp" >
                            </com.br.holding5.sc003.gallery.DeleteImageView>

                            <com.br.holding5.sc003.gallery.DeleteImageView
                                android:id="@+id/photo_view_5"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginRight="@dimen/dimen5dp" >
                            </com.br.holding5.sc003.gallery.DeleteImageView>

                            <com.br.holding5.sc003.gallery.DeleteImageView
                                android:id="@+id/photo_view_6"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginRight="@dimen/dimen5dp" >
                            </com.br.holding5.sc003.gallery.DeleteImageView>

                            <com.br.holding5.sc003.gallery.DeleteImageView
                                android:id="@+id/photo_view_7"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginRight="@dimen/dimen5dp" >
                            </com.br.holding5.sc003.gallery.DeleteImageView>
                        </LinearLayout>
                    </HorizontalScrollView>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ico_group_line" />

                    <RelativeLayout
                        android:id="@+id/add_photo_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/content_box_bg_center" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true" >

                            <ImageButton
                                android:id="@+id/add_picture_btn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@color/clear"
                                android:src="@drawable/selector_add_menu2" />

                            <ImageButton
                                android:id="@+id/crop_add_picture_btn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@color/clear"
                                android:src="@drawable/selector_add_menu1" />

                            <ImageButton
                                android:id="@+id/add_video_btn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@color/clear"
                                android:src="@drawable/selector_add_menu4" />

                            <ImageButton
                                android:id="@+id/capture_add_video_btn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@color/clear"
                                android:src="@drawable/selector_add_menu5" />

                            <ImageButton
                                android:id="@+id/add_audio_btn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@color/clear"
                                android:src="@drawable/selector_add_menu3"
                                android:visibility="visible" />
                        </LinearLayout>
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ico_group_line" />

                    <RelativeLayout
                        android:id="@+id/add_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:background="@drawable/content_box_bg_bottom"
                        android:paddingBottom="@dimen/dimen10dp"
                        android:paddingTop="@dimen/dimen10dp" >

                        <CheckBox
                            android:id="@+id/unknown_check"
                            style="@style/search_radio_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dimen50dp"
                            android:button="@drawable/selector_main_search_check"
                            android:text="익명" />

                        <ImageButton
                            android:id="@+id/board_content_add_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dimen50dp"
                            android:background="@color/clear"
                            android:src="@drawable/selector_register_btn" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/modify_btn_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/dimen10dp"
                        android:paddingTop="@dimen/dimen10dp"
                        android:visibility="gone" >

                        <ImageButton
                            android:id="@+id/board_modify_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:background="@color/clear"
                            android:src="@drawable/selector_btn_modify" />
                    </RelativeLayout>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/linearLayout_loading"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#66000000"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone" >

        <ProgressBar
            android:id="@+id/progressBar1"
            style="@style/TransDialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</RelativeLayout>