<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/title_bg" >

        <ImageView
            android:id="@+id/header_back_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:clickable="true"
            android:padding="5dp"
            android:src="@drawable/s00116_back_button" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:gravity="center" >

            <ImageView
                android:id="@+id/header_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/title_logo_img" />

            <TextView
                android:id="@+id/header_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#818181" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dip"
        android:background="@drawable/content_box_bg" >

        <FrameLayout
            android:id="@+id/profileimglayout"
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dip" >

            <ImageView
                android:id="@+id/profileimg"
                android:layout_width="60dip"
                android:layout_height="60dip"
                android:scaleType="fitXY"
                android:layout_centerHorizontal="true" />

            <ImageView
                android:id="@+id/frameimg"
                android:layout_width="60dip"
                android:layout_height="60dip"
                android:layout_centerHorizontal="true"
                android:background="@drawable/help_prf_bg" />
        </FrameLayout>

        <RelativeLayout
            android:id="@+id/profile1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/profileimglayout"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip"
            android:layout_marginTop="20dip"
            android:background="@drawable/input_box_bg" >

            <ImageView
                android:id="@+id/modifybtn1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/help_prf_modify_btn"
                android:visibility="gone" />

            <TextView
                android:id="@+id/textview1"
                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="생년월일" />

            <TextView
                android:id="@+id/edittext1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/modifybtn1"
                android:layout_toRightOf="@id/textview1"
                android:background="#00000000"
                android:text=""
                android:textColor="#000"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/profile1"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip"
            android:layout_marginTop="10dip"
            android:background="@drawable/input_box_bg" >

            <ImageView
                android:id="@+id/modifybtn2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/help_prf_modify_btn"
                android:visibility="gone" />

            <TextView
                android:id="@+id/textview2"
                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="직업" />

            <TextView
                android:id="@+id/edittext2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/modifybtn2"
                android:layout_toRightOf="@id/textview2"
                android:background="#00000000"
                android:password="true"
                android:text=""
                  android:textColor="#000"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/profile2"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip"
            android:layout_marginTop="10dip"
            android:background="@drawable/input_box_bg" >

            <ImageView
                android:id="@+id/modifybtn3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/help_prf_modify_btn"
                android:visibility="gone" />

            <TextView
                android:id="@+id/textview3"
                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="지역" />

            <TextView
                android:id="@+id/edittext3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/modifybtn3"
                android:layout_toRightOf="@id/textview3"
                android:password="true"
                android:text=""
                  android:textColor="#000"
                android:textSize="14sp"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile4"
            android:layout_width="match_parent"
            android:layout_height="180dip"
            android:layout_below="@id/profile3"
            android:layout_marginBottom="10dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip"
            android:layout_marginTop="10dip"
            android:background="@drawable/input_box_bg" >

            <TextView
                android:id="@+id/textview4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="10dip"
                   android:textColor="#000"
                android:textSize="12sp" />
        </RelativeLayout>
    </RelativeLayout>

</LinearLayout>