<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="TransDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowAnimationStyle">@+android:style/Animation.Dialog</item>
        <item name="android:windowBackground">@+android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    
    <!--  상세-->
    <style name="top_detail_common">
        <item name="android:textSize">@dimen/dimen27pt</item>
        <item name="android:textColor">@color/color_818181</item>
    </style>
     <style name="detail_nick">
        <item name="android:textSize">@dimen/dimen23pt</item>
        <item name="android:textColor">@color/color_3e3e3f</item>
    </style>
     <style name="detail_content">
        <item name="android:textSize">@dimen/dimen21pt</item>
        <item name="android:textColor">@color/color_505051</item>
    </style>
     <style name="detail_reply_menu">
        <item name="android:textSize">@dimen/dimen17pt</item>
        <item name="android:textColor">@color/color_999999</item>
    </style>
     <style name="detail_record_input">
        <item name="android:textSize">@dimen/dimen17pt</item>
        <item name="android:textColor">@color/color_979899</item>
    </style>
         <style name="detail_unknown">
        <item name="android:textSize">@dimen/dimen21pt</item>
        <item name="android:textColor">@color/color_505051</item>
    </style>
    
     
    
    <!-- 리스트 제목 -->
    <style name="list_title_text">
        <item name="android:textSize">@dimen/dimen23pt</item>
        <item name="android:textColor">@color/color_3e3e3f</item>
    </style>

    <!-- 리스트 카테고리 -->
    <style name="list_category_text">
        <item name="android:textSize">@dimen/dimen12sp</item>
        <item name="android:textColor">@color/color_999999</item>
    </style>

    <!-- 리스트 상태값 -->
    <style name="list_stastuc_text">
        <item name="android:textSize">@dimen/dimen17pt</item>
        <item name="android:textColor">#999999</item>
    </style>

    <!-- 라디오버튼 텍스트1 -->
    <style name="search_radio_text">
        <item name="android:textSize">@dimen/dimen21pt</item>
        <item name="android:textColor">#505051</item>
        <item name="android:paddingLeft">@dimen/dimen5dp</item>
    </style>

    <!-- 리스트 작성시간 -->
    <style name="list_create_time_text">
        <item name="android:textSize">@dimen/dimen12sp</item>
        <item name="android:textColor">@color/color_505051</item>
    </style>
    <!-- 리스트 콘텐츠 -->
    <style name="list_contents_text">
        <item name="android:textSize">@dimen/dimen21pt</item>
        <item name="android:textColor">@color/color_505051</item>
    </style>

    <!-- 글쓰기 내용 -->
    <style name="write_contents_text">
        <item name="android:textSize">@dimen/dimen19pt</item>
        <item name="android:textColor">@color/color_979899</item>
    </style>

    <!-- 글쓰기 메뉴 -->
    <style name="write_menu_text">
